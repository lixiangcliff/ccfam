container_commands:
    01_migrate:
        command: "python3 src/manage.py migrate --noinput"
        leader_only: true

    02_makesuper:
        command: "python3 src/manage.py makesuper"
        leader_only: true

    03_collectstatic:
        command: "python3 src/manage.py collectstatic --noinput"
        leader_only: true